<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
      "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="hibernate.deudaAsistente.DeudaAsistente" table="DEUDA">
		<id name="id" type="int">
            <column name="Id"/>
            <generator class="increment" />            
        </id>
		<property name="prestamista" type="string">
			<column name="Prestamista"/>
		</property>
		<property name="deudor" type="string">
			<column name="Deudor"/>
		</property>
		<property name="valor" type="int">
			<column name="Valor"/>
		</property>
	</class>
</hibernate-mapping>